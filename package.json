    "rimraf": "2.6.2",
    "selenium-server": "3.12.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.5.9",
    "vue-jest": "1.4.0",
    "vue-loader": "13.7.2",
    "vue-style-loader": "3.1.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-server": "2.11.2",
    "webpack-merge": "4.1.3"
  },
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
