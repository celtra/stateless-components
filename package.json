{
  "name": "stateless-components",
  "version": "1.0.0",
  "description": "Vue.js stateless components source of truth and documentation",
  "author": "zan <zan.kusterle@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs && stylelint '**/*.vue'",
    "build": "node build/build-library.js",
    "heroku-postbuild": "node build/build.js",
    "start": "node server.js",
    "git-add": "git add index.js index.js.map"
  },
  "pre-commit": [
    "build",
    "git-add"
  ],
  "dependencies": {
    "express": "4.16.3",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "moment": "2.22.2",
    "vue": "2.5.16",
    "vue-router": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "chromedriver": "2.40.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-vue": "4.5.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.4.4",
    "jest-serializer-vue": "0.3.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "nightwatch": "0.9.21",
    "node-notifier": "5.2.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "ora": "1.4.0",
    "portfinder": "1.0.13",
